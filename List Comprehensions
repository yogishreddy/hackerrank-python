if __name__ == '__main__':
    x = int(input())
    y = int(input())
    z = int(input())
    n = int(input())
def cl(x,y,z,n,l):
    t=[]
    for i in range(0,x+1):
        for j in range(0,y+1):
            for k in range(0,z+1):
                if i+j+k != n:
                    t.append(i)
                    t.append(j)
                    t.append(k)
                    l.append(t)
                    t=[]
    return l
l=[]
print(cl(x,y,z,n,l))                    
