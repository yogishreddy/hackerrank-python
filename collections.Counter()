from collections import Counter
def costcal(sizes,noc):
    sum=0
    while noc:
        s,v=map(int,input().split())
        if s in Counter(sizes).keys():
            sum+=v
            sizes.remove(s)
        noc-=1
    return sum
n = int(input())
sizes = list(map(int,input().split()[:n]))
noc= int(input())
print(costcal(sizes,noc))
